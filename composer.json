{
	"name": "rob006/yii-elfinder2",
	"description": "Extension to use elFinder 2.0 file manager in yii application",
	"keywords": [
		"widget",
		"tinymce",
		"file manager",
		"elFinder"
	],
	"type": "yii-extension",
	"homepage": "https://github.com/rob006-software/yii-elfinder2",
	"support": {
		"issues": "https://github.com/rob006-software/yii-elfinder2/issues",
		"source": "https://github.com/rob006-software/yii-elfinder2"
	},
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Robert Korulczyk",
			"email": "robert@korulczyk.pl",
			"homepage": "https://rob006.net/"
		},
		{
			"name": "Bogdan Savluk",
			"email": "savluk.bogdan@gmail.com"
		}
	],
	"require": {
		"yiisoft/yii": "~1.1.18"
	},
	"replace": {
		"studio-42/elfinder": "2.0.9"
	},
	"autoload": {
		"classmap": [
			"."
		]
	},
	"suggest": {
		"rob006/yii-tinymce": "Provides WYSIWYG editor integration."
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.1-dev"
		}
	}
